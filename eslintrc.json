{
  "extends": [
    "eslint:recommanded",
    "plugin:import/errors",
    "plugin:react/recommanded",
    "plugin:jsx-a11y/recommanded",
    "prettier",
    "prettier/react"
  ],
  "rules": {
    "react/prop-types": 0,
    "no-console": 1,
    "react-hooks/rules-of-hooks": 2,
    "react-hooks/exhustive-deps": 1
  },
  "plugins": ["react", "import", "jsx-a11y", "react-hooks"],
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true,
    "jquery": true
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "globals": {
    "$": true
  }
}
